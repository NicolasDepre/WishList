<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.LaPireTeam.wishlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Activity -->
        <activity android:name=".NewWishActivity"/>
        <activity android:name=".NewListActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".MyListsActivity"/>
        <activity android:name=".AmisActivity"/>
        <activity android:name=".SeeWishesActivity"/>
        <activity android:name=".ProfilActivity"/>
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>